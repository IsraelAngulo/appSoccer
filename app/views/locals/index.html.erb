<h1>Listing locals</h1>

<table>
  <tr>
    <th>Descriptivenamelocal</th>
    <th>Address</th>
    <th>Addressingooglemaps</th>
    <th>Telephone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @locals.each do |local| %>
  <tr>
    <td><%= local.descriptiveNameLocal %></td>
	<td><%= local.district.name %></td>
    <td><%= local.address %></td>
    <td><%= local.addressinGoogleMaps %></td>
    <td><%= local.telephone %></td>
    <td><%= link_to 'Show', local %></td>
    <td><%= link_to 'Edit', edit_local_path(local) %></td>
    <td><%= link_to 'Destroy', local, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Local', new_local_path %>
