<h1>Listing photoalbums</h1>

<table>
  <tr>
	<th>Field</th>
    <th>Name</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photoalbums.each do |photoalbum| %>
  <tr>
	<td><%= photoalbum.field.name %></td>
    <td><%= photoalbum.name %></td>
    <td><%= photoalbum.date %></td>
    <td><%= link_to 'Show', photoalbum %></td>
    <td><%= link_to 'Edit', edit_photoalbum_path(photoalbum) %></td>
    <td><%= link_to 'Destroy', photoalbum, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photoalbum', new_photoalbum_path %>
